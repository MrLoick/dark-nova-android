<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent">
  
<TextView android:text="Deposit Cash"
 android:id="@+id/Seek1heading" 
 android:textSize="13dp"
 android:layout_marginTop="20dp"
 android:layout_marginLeft="20dp"
 android:textStyle="bold"
 android:layout_width="wrap_content" 
 android:layout_height="wrap_content"></TextView>
 
<TextView android:text="How much you want to deposit "
 android:id="@+id/Seek1Line1" 
 android:textSize="11dp"
 android:layout_marginTop="10dp"
 android:layout_marginLeft="10dp"
 android:layout_width="wrap_content" 
 android:layout_height="wrap_content"></TextView>
 
<TextView android:text="into your GSEDB ?"
 android:id="@+id/Seek1Line2"
 android:textSize="11dp"
 android:layout_marginLeft="30dp"
  android:layout_width="wrap_content"
   android:layout_height="wrap_content"></TextView>
   
   <LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="horizontal"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content">
   
<TextView android:text="Selected amount:" 
android:id="@+id/Seek1Line3" 
android:textSize="11dp"
 android:layout_marginLeft="30dp"
android:layout_width="wrap_content" 
android:layout_height="wrap_content"></TextView>


<TextView android:text="111"
 android:id="@+id/Seek1Alertvalue" 
 android:textSize="11dp"
 android:layout_width="wrap_content" 
 android:layout_height="wrap_content"></TextView>
 
 </LinearLayout>
   <LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="horizontal"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content">

<Button android:text="Cancel" 
android:id="@+id/Seek1alertno"
 android:layout_width="wrap_content" 
 android:layout_height="wrap_content"></Button>
<Button android:text="Deposit"
 android:id="@+id/Seek1alertyes"
  android:layout_width="wrap_content" 
  android:layout_height="wrap_content"></Button>
  </LinearLayout>
</LinearLayout>
