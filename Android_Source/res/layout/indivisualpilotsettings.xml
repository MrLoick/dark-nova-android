<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content">
  
  <LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="horizontal"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content">
  
<TextView android:text="Full Tank on Arrival"
 android:id="@+id/TextView01" 
 android:layout_marginLeft="20dp"
 android:layout_marginTop="25dp"
 android:textSize="15dp"
 android:layout_width="wrap_content"
android:layout_height="wrap_content"></TextView>

<Button android:text="ON"
 android:id="@+id/FullTankonArrivalButtonon" 
 android:layout_marginTop="25dp"
 android:layout_marginLeft="35dp"
 android:layout_width="wrap_content" 
 android:layout_height="wrap_content"></Button>
 
<Button android:text="OFF" 
android:id="@+id/FullTankonArrivalButtonoff" 
android:layout_marginTop="25dp"
android:layout_width="wrap_content" 
android:layout_height="wrap_content"></Button>

</LinearLayout>
  
   <LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="horizontal"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content">
  
<TextView android:text="Hull Repair on Arrival"
 android:id="@+id/TextView02" 
 android:layout_marginLeft="20dp"
 android:layout_marginTop="05dp"
 android:textSize="15dp"
 android:layout_width="wrap_content"
android:layout_height="wrap_content"></TextView>

<Button android:text="ON"
 android:id="@+id/HullRepaironArrivalButtonon" 
 android:layout_marginTop="05dp"
 android:layout_marginLeft="22dp"
 android:layout_width="wrap_content" 
 android:layout_height="wrap_content"></Button>
 
<Button android:text="OFF" 
android:id="@+id/HullRepaironArrivalButtonoff" 
android:layout_marginTop="05dp"
android:layout_width="wrap_content" 
android:layout_height="wrap_content"></Button>

</LinearLayout>
  
  <LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="horizontal"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content">
  
<TextView android:text="Always Buy News"
 android:id="@+id/TextView03" 
 android:layout_marginLeft="20dp"
 android:layout_marginTop="05dp"
 android:textSize="15dp"
 android:layout_width="wrap_content"
android:layout_height="wrap_content"></TextView>

<Button android:text="ON"
 android:id="@+id/AlwaysBuyNewson" 
 android:layout_marginTop="05dp"
 android:layout_marginLeft="50dp"
 android:layout_width="wrap_content" 
 android:layout_height="wrap_content"></Button>
 
<Button android:text="OFF" 
android:id="@+id/AlwaysBuyNewsoff" 
android:layout_marginTop="05dp"
android:layout_width="wrap_content" 
android:layout_height="wrap_content"></Button>

</LinearLayout>
  
  <LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="horizontal"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content">
  
<TextView android:text="Loan Reminders"
 android:id="@+id/TextView04" 
 android:layout_marginLeft="20dp"
 android:layout_marginTop="05dp"
 android:textSize="15dp"
 android:layout_width="wrap_content"
android:layout_height="wrap_content"></TextView>

<Button android:text="ON"
 android:id="@+id/LoanReminderson" 
 android:layout_marginTop="05dp"
 android:layout_marginLeft="57dp"
 android:layout_width="wrap_content" 
 android:layout_height="wrap_content"></Button>
 
<Button android:text="OFF" 
android:id="@+id/LoanRemindersoff" 
android:layout_marginTop="05dp"
android:layout_width="wrap_content" 
android:layout_height="wrap_content"></Button>

</LinearLayout>
  
   <LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="horizontal"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content">
  
<TextView android:text="Reserve Money to Warp"
 android:id="@+id/TextView05" 
 android:layout_marginLeft="20dp"
 android:layout_marginTop="35dp"
 android:textSize="15dp"
 android:layout_width="wrap_content"
android:layout_height="wrap_content"></TextView>

<Button android:text="ON"
 android:id="@+id/Button09" 
 android:layout_marginTop="35dp"
 android:layout_marginLeft="35dp"
 android:layout_width="wrap_content" 
 android:layout_height="wrap_content"></Button>
 
<Button android:text="OFF" 
android:id="@+id/Button10" 
android:layout_marginTop="35dp"
android:layout_width="wrap_content" 
android:layout_height="wrap_content"></Button>



</LinearLayout>
  <SeekBar 
android:id="@+id/SeekBarvolume" 
android:layout_height="20dp"
android:layout_width="150dp"
android:layout_marginTop="12dp"
android:layout_marginLeft="10dp"
android:max="100"
android:progress="2"
android:gravity="center">
</SeekBar>
  
</LinearLayout>
